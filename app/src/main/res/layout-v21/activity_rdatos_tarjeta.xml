<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.erickcairdiel.koko.RDatosTarjeta">

    <TextView
        android:id="@+id/txt_Ingrese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:text="@string/ingrese_los_datos_de_su_tarjeta"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="64dp"
        tools:ignore="RtlHardcoded" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_numtarjeta"
        android:layout_marginLeft="16dp"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/txt_numtarjeta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/numero_de_tarjeta_16_digitos"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="152dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/c_digo_de_seguridad"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="192dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="190dp"
        android:layout_height="30dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="256dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="300dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="56dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/caducidad"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="268dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="@string/a_o"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        android:layout_marginLeft="28dp"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="@string/mes"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        android:layout_marginLeft="36dp"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/btn_regresar"
        android:layout_width="68dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="32dp"
        android:backgroundTint="#ffffff"
        android:drawableLeft="@drawable/regresar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_continuar"
        app:layout_constraintHorizontal_bias="0.162"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="lollipop"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="32dp"
        android:drawableStart="@drawable/regresar"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/btn_continuar"
        android:layout_width="68dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="40dp"
        android:backgroundTint="#ffffff"
        android:drawableLeft="@drawable/continuar"
        android:drawableStart="@drawable/continuar"
        android:text="@string/btn_Continuar"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:targetApi="lollipop"
        android:layout_marginRight="40dp"
        tools:ignore="RtlHardcoded" />

    <CheckBox
        android:id="@+id/checkBox_tarjeta1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:backgroundTint="#ffffff"
        android:drawableLeft="@drawable/masterC"
        android:drawableStart="@drawable/masterC"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="73dp"
        android:layout_marginLeft="40dp"
        tools:ignore="MissingConstraints,RtlHardcoded" />

    <CheckBox
        android:id="@+id/checkBox_tarjeta2"
        android:layout_width="wrap_content"
        android:layout_height="73dp"
        android:layout_marginStart="8dp"
        android:backgroundTint="#ffffff"
        android:drawableLeft="@drawable/visaT"
        android:drawableStart="@drawable/visaT"
        app:layout_constraintStart_toEndOf="@+id/checkBox_tarjeta1"
        tools:layout_editor_absoluteY="69dp"
        android:layout_marginLeft="8dp"
        tools:ignore="MissingConstraints,RtlHardcoded" />

    <CheckBox
        android:id="@+id/checkBox_tarjeta3"
        android:layout_width="wrap_content"
        android:layout_height="67dp"
        android:layout_marginStart="8dp"
        android:backgroundTint="#ffffff"
        android:drawableLeft="@drawable/americanT"
        android:drawableStart="@drawable/americanT"
        app:layout_constraintStart_toEndOf="@+id/checkBox_tarjeta2"
        tools:layout_editor_absoluteY="73dp"
        android:layout_marginLeft="8dp"
        tools:ignore="MissingConstraints,RtlHardcoded" />

</android.support.constraint.ConstraintLayout>
